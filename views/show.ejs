<%- include("partials/header.ejs") %>

<h1> <%= print.name %> </h1> 
<img src="<%= print.url %>" >
<p> Artist: <%= print.Artist %> </p>
<h2> Comments: </h2>
<% print.comments.forEach(function(comment){ %>
    <p>
        <strong><%= comment.author %></strong> - <%= comment.text %>
    </p>
<% }) %>

<%- include("partials/footer.ejs") %>