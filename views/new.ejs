<%- include("partials/header.ejs") %>

<div class="container">
<h1>Add your new print</h1>
<form action="/prints" method="POST">
<div class="mb-3">
    <label class="form-label">Title</label>
    <input type="text" class="form-control" name="name" placeholder="Print title">
</div>
<div class="mb-3">
    <label  class="form-label">Artist</label>
    <input type="text" class="form-control" name="Artist" placeholder="Artist name">
</div>
<div class="mb-3">
    <label  class="form-label">URL</label>
    <input type="text" class="form-control" name="url" placeholder="Image URL">
</div>
<button type="submit" class="btn btn-primary">Submit</button>
<a href="/prints">Go back</a>
</form>
</div>

<%- include("partials/footer.ejs") %>